            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.MenuRefCompraActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.tipocomprobante.TipoComprobanteAddActivity"
